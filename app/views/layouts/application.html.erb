    <![endif]-->

</head>

<body ng-controller="StoreController">

  <h3> {{name}}</h3>


    <div id="wrapper">

      <%= render 'layouts/side_menu' %>
        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <%= yield %>
                </div>
